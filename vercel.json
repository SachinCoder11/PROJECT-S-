{
    "version": 2,
    "builds": [
      {
        "src": "index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.php"
      }
    ],
    "env": {
      "YOUR_ENV_VARIABLE": "your_value"
    },
    "regions": ["all"]
  }
  